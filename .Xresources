Xft.dpi:       116
Xft.antialias: true
Xft.hinting:   true
Xft.rgba:      rgb
Xft.autohint:  false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault


URxvt.depth:                      32
URxvt.background:                 #121219
URxvt*scrollBar:                  false
URxvt*mouseWheelScrollPage:       false
URxvt*cursorBlink:                true
URxvt*foreground:                 grey
URxvt*saveLines:                  5000
URxvt*letterSpace:                1 
URxvt*internalBorder:             8
URxvt*externalBorder:             0


URxvt.font:     xft:Hack Nerd Font Mono:style=Regular:pixelsize=14

! main colors {{{
! black
*.color0:       #121110
*.color8:       #2D2C29

! red
*.color1:       #ef2f27
*.color9:       #f75341

! green
*.color2:       #03E900
*.color10:      #98bc37

! yellow
*.color3:       #D5AB00
*.color11:      #fed06e

! blue
*.color4:       #0300A1
*.color12:      #68A8E4

! magenta
*.color5:       #e02c6d
*.color13:      #ff5c8f

! cyan
*.color6:       #0aaeb3
*.color14:      #53fde9

! white
*.color7:       #918175
*.color15:      #fce8c3


